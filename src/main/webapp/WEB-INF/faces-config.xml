 	<navigation-rule>
 		<from-view-id>/cliente-list.xhtml</from-view-id>
 		<navigation-case>
 			<from-outcome>formulario</from-outcome>
 			<to-view-id>/cliente-form.xhtml</to-view-id>
 			<redirect />
 		</navigation-case>
 	</navigation-rule>
 	<navigation-rule>
 		<from-view-id>/cliente-form.xhtml</from-view-id>
 		<navigation-case>
 			<from-outcome>lista</from-outcome>
